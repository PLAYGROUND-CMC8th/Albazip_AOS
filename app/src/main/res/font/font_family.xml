<?xml version="1.0" encoding="utf-8"?>
<font-family
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!--regular-->
    <font
        android:font="@font/roboto_r"
        android:fontStyle="normal"
        android:fontWeight="400"
        app:font="@font/roboto_r"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- medium -->
    <font
        android:font="@font/roboto_m"
        android:fontStyle="normal"
        android:fontWeight="500"
        app:font="@font/roboto_m"
        app:fontStyle="normal"
        app:fontWeight="500"/>

    <!--bold-->
    <font
        android:font="@font/roboto_b"
        android:fontStyle="normal"
        android:fontWeight="700"
        app:font="@font/roboto_b"
        app:fontStyle="normal"
        app:fontWeight="700" />


</font-family>